{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "gdcvm",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/src/cmd/gdcvm/main.go",
      "args": [
        "-pkg",
        "${workspaceFolder}/examples/playground"
      ],
      "showLog": false,
      "buildFlags": "-tags=debug"
    },
    {
      "name": "gdc",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/src/cmd/gdc/main.go",
      "args": [
        "-pkg",
        "${workspaceFolder}/examples/playground"
      ],
      "showLog": false
    },
    {
      "name": "gdc [Debug]",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/src/cmd/gdc/main.go",
      "args": [
        "-pkg",
        "${workspaceFolder}/examples/playground"
      ],
      "showLog": false,
      "buildFlags": "-tags=debug"
    },
    {
      "name": "gdvm",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/src/cmd/gdvm/main.go",
      "args": [
        "-gdbin",
        "${workspaceFolder}/examples/playground/playground.gdbin"
      ],
      "showLog": false,
      "buildFlags": "-tags=debug"
    }
  ]
}